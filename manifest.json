{
  "manifest_version": 2,

  "name": "Youtube Peek",
  "description": "Lets you preview the video, in the form of a storyboard slideshow by hovering over the video thumbnail",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.youtube.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs", 
    "http://www.youtube.com/*"
    "activeTab"
  ]
}
